# For local dev: can be same as DATABASE_URL
DIRECT_DATABASE_URL="postgresql://postgres@localhost:5432/rcfa_v1?schema=public"
OPENAI_API_KEY=""
OPENAI_MODEL="gpt-4o"
AUTH_TOKEN_SECRET=""
